program
{

	proc Sum : float ( v1 : float, v2 : float )
	{
	    return v1 + v2;
	}

	proc Main()
	{
	    proc InnerSum : int ( v1 : int , v2 : int )
	    {
	        return v1 + v2;
	    }
	    var f : float;
	    var v : int;
	    v := InnerSum( 40, 2 );
	    f := Sum( 41.5, 0.5 );
	    printfloat( f );
	    printint( v );
	}

}